{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CacheConfig": {
    "DefaultCacheTime": 60,
    "ShortTermCacheTime": 3,
    "BundledFilesCacheTime": 120
  },
  "DistributedCacheConfig": {
    "DistributedCacheType": "redis",
    "Enabled": true,
    "ConnectionString": "127.0.0.1:6379,ssl=False",
    "SchemaName": "dbo",
    "TableName": "DistributedCache"
  },
  "OpenTelemetry": {
    "Metrics": {
      "Enabled": true,
      "AspNetCoreInstrumentation": true,
      "HttpClientInstrumentation": true,
      "RuntimeInstrumentation": true,
      "ProcessInstrumentation": true
    },
    "Tracing": {
      "Enabled": true,
      "AspNetCoreInstrumentation": true,
      "HttpClientInstrumentation": true,
      "SqlClientInstrumentation": true,
      "OtlpExporterEndpoint": "http://otel-collector:4317"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Clean Template Project Service",
        "Uri": "https://localhost:44398/health"
      }
    ],
    "EvaluationTimeInSeconds": 5
  }
}